<div id="app">
  {{ message }}

  <div id="code">
    <div class="text-container">
      <textarea rows="15" cols="50" v-model="code">
      </textarea>
    </div>
    <div class="text-container2" v-bind:class="{ active: isActive}">
        <textarea class="textarea2" rows="15" cols="50" v-model="code2">
        </textarea>
    </div>
    <div class="button-container">
      <div>
        <i v-on:click="toggleTextbox()" id="icon-white" class="material-icons">add</i>
      </div>
      <div>
        <i v-on:click="analyzeCode()" class="material-icons">settings</i>
      </div>
    </div>
    

      
  </div>
  <div id="result">
    <p>Number Of Steps: {{ results }}</p>
  </div>

  <line-chart :chart-data="chartData" :options="options" :width="1000"></line-chart>
</div>